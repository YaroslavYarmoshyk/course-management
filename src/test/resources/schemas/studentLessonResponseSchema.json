{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Generated schema for Root",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "lessonId": {
        "type": "number"
      },
      "title": {
        "type": "string"
      },
      "description": {
        "type": "string"
      },
      "lessonContent": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "type": "number"
            },
            "lessonPart": {
              "type": "string"
            },
            "fileId": {
              "type": "number"
            }
          },
          "required": [
            "id",
            "lessonPart",
            "fileId"
          ]
        }
      },
      "markValue": {
        "type": "number"
      },
      "mark": {
        "type": "string"
      }
    },
    "required": [
      "lessonId",
      "title",
      "description",
      "markValue",
      "mark"
    ]
  }
}
